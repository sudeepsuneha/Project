pipeline {
        agent any
        stages {
            stage(build) {
                steps {   sh 'mvn clean install'  }
            }
            stage(test) {
                steps {  sh 'mvn test' }
                post {
                    always {
                        junit 'target/surefire-reports/*.xml'
                        archiveArtifacts 'target/*.jar'
                    }
                }
            }
            }
        }
